{
  "name": "nuxt-vanilla-extract",
  "type": "module",
  "version": "0.1.1",
  "description": "Zero-runtime stylesheets in your Nuxt 3 apps.",
  "keywords": [
    "css",
    "css-in-js",
    "css-in-ts",
    "vue",
    "nuxt",
    "vanilla",
    "emotion"
  ],
  "license": "MIT",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wobsoriano/nuxt-vanilla-extract.git"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm prepack",
    "lint": "eslint .",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@emotion/hash": "^0.9.0",
    "pathe": "^0.3.9",
    "@nuxt/kit": "^3.0.0-rc.12",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/integration": "^6.0.0",
    "@vanilla-extract/vite-plugin": "^3.6.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "^3.0.0-rc.12",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.14.0",
    "nuxt": "^3.0.0-rc.12"
  }
}
